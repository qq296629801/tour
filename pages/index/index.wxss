/**index.wxss**/
.box{
  box-sizing: border-box;
  width: 100%;
  height: 90rpx;
  padding: 0 28rpx;
  background: #00736d;
  display: flex;
  align-items: center;
  justify-content: space-between;
  color: #fff;
}
.loca {
  display: flex;
  align-items: center;
}
.loca text {
  font-size: 28rpx;
  padding-left: 6rpx;
}
.loca image{
  width: 28rpx;
  height: 32rpx;
}
.box .search-bar{
  background: #00746c;
  color:#fff;
  position: relative;
}
.box .search-bar image{
  width: 28rpx;
  height: 28rpx;
  position: absolute;
  top: 16rpx;
  left: 16rpx;
}
.box .search-bar-txt{
  width: 530rpx;
  background: #32908a;
  border-radius: 30rpx;
  color:#fff;
  height: 60rpx;
  padding-left: 50rpx;
  font-size: 22rpx;
}

.banner{
  width: 100%;
  height:  350rpx;
  border: none;
}
.news-box {
  height: 89rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 0 26rpx;
  box-sizing: border-box;
}
.news {
  width: 100%;
  height: 68rpx;
  box-shadow: 0  0 1rpx 0 rgba(0, 0, 0, 0.6);
  border-radius: 20rpx;
  line-height: 68rpx;
  padding-left: 30rpx;
  padding-right: 10rpx;
  display: flex;
  position: relative;
  background: #fff;
}
.news-con {
  width: 72%;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.more {
  position: absolute;
  right: 10rpx;
}
.news-tips {
  color: #47a86c;
}
.icon-box{
	display: flex;
	flex-wrap: wrap;
	background: #fff;
  margin-bottom: 10rpx;
}
.icon-item{
  width: 33.3%;
  padding: 20rpx 0;
	display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  color: #696969;
}
.icon-item image{
	width: 140rpx;
	height: 140rpx;
}
.icon-item text{
  margin-top: -10rpx;
	font-size: 24rpx;
}
.tuijian {
  display: flex;
  flex-wrap: wrap;
  background: #fff;
  padding: 0 10rpx;
}
.tuijian-box {
  font-size: 24rpx;
  color: #a6a6a6;
  margin: 0;
}
.img {
  margin: 10rpx 0;
  width: 98% !important;
  height: 185rpx !important;
}